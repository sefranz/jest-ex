{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-props-no-spreading": "off",
        "react/forbid-prop-types": "off",
        "no-plusplus": "off",
        "import/prefer-default-export": "off",
        "no-param-reassign": "off",
        "no-unused-vars": "warn",
        "no-underscore-dangle": "off",
        "react/no-array-index-key": "off",
        "import/no-cycle": "off"
    }
}
